{
	"name": "kedniko/vivy",
	"description": "PHP validator",
	"autoload": {
		"psr-4": {
			"Kedniko\\Vivy\\": "src/",
			"Kedniko\\Vivy\\Call\\": "src/.svn/",
			"App\\": "app/",
			"Tests\\": "Tests/"
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"kedniko/php-ide-helper": "dev-main",
		"nikic/php-parser": "^4.15",
		"brick/math": "^0.11.0"
	},
	"require-dev": {
		"pestphp/pest": "^1.22",
		"fakerphp/faker": "^1.21"
	},
	"repositories": [
		{
			"type": "path",
			"url": "D:/repositories/kedniko/php-ide-helper"
		}
	],
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}